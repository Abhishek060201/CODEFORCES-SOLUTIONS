#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std ;
#define siz 100001
#define mod 1000000007
int main()
{ 
      z n,i,fl=0,j,k,count=0;
      sf("%lld",&n);
      string  s,t ;
      cin>>s>>t;
      vector <int> v;

      for(i=0;i<n;i++)
      {
        fl=0;
            if(s[i]==t[i])
            {
                fl=1;
                continue;
            }
            else
            {
                for(j=i;j<n;j++)
                {
                    if(s[j]==t[i])
                    {
                      fl=1;
                        for(k=j;k>i;k--)
                        {
                          swap(s[k],s[k-1]);
                        v.push_back(k);
                        count++;
                      }
                    }
                }

            }
            if(fl==0)
            {
              cout << "-1" ;
              return 0;
            }
      }

      if(!fl)
        pf("-1");
      else
      {
          cout << count <<endl;
          if(count){
        for(i=0;i<v.size();i++)
          cout << v[i] <<" ";}


      }
}
