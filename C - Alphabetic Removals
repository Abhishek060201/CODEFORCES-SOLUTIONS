#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std;


int main() {

  
    int n,k,i,j;
    sf("%d%d",&n,&k);
      string s,st;
      cin>>s ;
      for(j=1;j<=26;j++)
      {
          char ch = 97 + j ;
      for(i=0;i<n;i++) 
      {
          if(s[i]==ch) {s[i]='0';k--;}
          if(k==0) break;
      }

    }


    for(i=0;i<n;i++)
    {
        if(s[i]!='0') st.push_back(s[i]);
    }
  
    cout<< st;
}
